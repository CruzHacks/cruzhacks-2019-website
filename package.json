{
  "name": "cruzhacks-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "6.3.0",
    "node": "v8.10.0"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "fetch-jsonp": "^1.1.3",
    "http-proxy-middleware": "^0.19.1",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "polished": "^2.3.3",
    "react": "^16.8.6",
    "react-countdown-now": "^2.1.0",
    "react-countup": "^4.1.3",
    "react-device-detect": "^1.6.2",
    "react-dom": "^16.8.6",
    "react-https-redirect": "^1.1.0",
    "react-lottie": "^1.2.3",
    "react-on-visible": "^1.6.0",
    "react-recaptcha-v3": "^1.1.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-s3": "^1.3.1",
    "react-scripts": "^3.0.1",
    "react-scroll-horizontal": "^1.6.2",
    "styled-components": "^4.2.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build": "cross-env NODE_ENV=production NODE_PATH=./src npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
